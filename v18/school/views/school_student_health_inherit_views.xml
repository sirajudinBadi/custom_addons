<?xml version="1.0" encoding="utf-8" ?>

<odoo>
    <record id="school_student_health_inherit_view" model="ir.ui.view">
        <field name="name">school.student.health.inherit.view</field>
        <field name="model">school.student</field>
        <field name="inherit_id" ref="school.school_student_form" />
        <field name="arch" type="xml">
            <xpath expr="//group[@name='file_group']" position="after">
                <group string="Health Information">
                    <field name="health_score" />
                    <field name="is_allergen" />
                </group>
            </xpath>
        </field>
    </record>
</odoo>