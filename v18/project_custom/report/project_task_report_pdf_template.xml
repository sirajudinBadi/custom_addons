<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <template id="task_report_template">
            <t t-call="web.html_container">
                <t t-foreach="docs" t-as="task">
                    <div class="page">
                        <h3>
                            <t t-esc="task.name"/>
                        </h3>
                        <p>Start:
                            <t t-esc="task.date_start"/>
                        </p>
                        <p>End:
                            <t t-esc="task.date_end"/>
                        </p>
                    </div>
                </t>
            </t>
        </template>

    </data>
</odoo>