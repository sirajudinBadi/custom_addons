<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="cron_timesheet_summary_pm" model="ir.cron">
        <field name="name">Send Timesheet Summary</field>
        <field name="model_id" ref="model_task_timesheet_remainder" />
        <field name="state">code</field>
        <field name="code">model.send_daily_project_timesheet()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="nextcall" eval="(DateTime.now() + timedelta(days=1)).strftime('%Y-%m-%d 04:30:00')" />
        <field name="active">True</field>
    </record>
</odoo>